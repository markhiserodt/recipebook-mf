{
  "name": "@recipebook-mf/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve recipebook --devRemotes=inventory --devRemotes=account",

    "build": "nx build recipebook && nx build inventory && nx build account",
    "recipebook:build:prod": "nx build recipebook --prod",
    "inventory:build:prod": "nx build inventory --prod",
    "account:build:prod": "nx build account --prod",

    "lint": "nx lint recipebook && nx lint inventory && nx lint account",
    "recipebook:lint": "nx lint recipebook",
    "inventory:lint": "nx lint inventory",
    "account:lint": "nx lint account",

    "test": "nx test recipebook && nx test inventory && nx test account",
    "recipebook:test:prod": "nx test recipebook --codeCoverage",
    "inventory:test:prod": "nx test inventory --codeCoverage",
    "account:test:prod": "nx test account --codeCoverage",

    "e2e": "nx e2e recipebook-e2e && nx e2e inventory-e2e && nx e2e account-e2e",
    "recipebook:e2e:prod": "nx e2e recipebook-e2e --watch=false",
    "inventory:e2e:prod": "nx e2e inventory-e2e --watch=false",
    "account:e2e:prod": "nx e2e account-e2e --watch=false",

    "coffee": "npm run build && npm run lint && npm run test && npm run e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.0.0",
    "@angular/common": "~17.0.0",
    "@angular/compiler": "~17.0.0",
    "@angular/core": "~17.0.0",
    "@angular/forms": "~17.0.0",
    "@angular/platform-browser": "~17.0.0",
    "@angular/platform-browser-dynamic": "~17.0.0",
    "@angular/router": "~17.0.0",
    "@azure/msal-angular": "^3.0.11",
    "@azure/msal-browser": "^3.7.1",
    "@nx/angular": "17.2.8",
    "bootstrap-icons": "^1.11.3",
    "msal": "^1.4.18",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.0.0",
    "@angular-devkit/core": "~17.0.0",
    "@angular-devkit/schematics": "~17.0.0",
    "@angular-eslint/eslint-plugin": "~17.0.0",
    "@angular-eslint/eslint-plugin-template": "~17.0.0",
    "@angular-eslint/template-parser": "~17.0.0",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "~17.0.0",
    "@angular/language-service": "~17.0.0",
    "@nx/cypress": "17.2.8",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/jest": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/web": "17.2.8",
    "@nx/webpack": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@schematics/angular": "~17.0.0",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "cypress": "^13.0.0",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.4",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.4",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "17.2.8",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.2.2"
  }
}
